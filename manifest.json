{
    "manifest_version": 3,
    "name": "SAT CopyPaste",
    "version": "1.0",
    "description": "A simple Chrome extension enable copy paste on SAT Portal.",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
        {
            "js": ["content-script.js"],
            "matches": ["https://portal.facturaelectronica.sat.gob.mx/*"],
            "all_frames": true,
            "run_at": "document_idle",
            "world": "MAIN"
        }
    ]
}
